{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": "standard",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"space-before-function-paren": "off",
		"semi": ["error", "always"],
		"no-tabs": ["error", { "allowIndentationTabs": true }],
		"indent": ["error", "tab"],
		"comma-dangle": ["error", "only-multiline"]
	}
}
